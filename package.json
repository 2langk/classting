{
  "name": "classting",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "ts-patch install && typia patch",
    "format": "prettier --write \"src/**/*.ts\" && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "node --env-file=config/dotenv/.env.production dist/main.js",
    "dev": "tsnd --env-file=config/dotenv/.env.local --respawn --rs src/main.ts",
    "gen:env": "dotenv-const -f ./config/dotenv/.env.local -o ./src/@libs/common/environment.ts",
    "gen:enum": "tsnd --env-file=config/dotenv/.env.local config/script/generate-config-enum.ts",
    "gen:prisma": "tsnd --env-file=config/dotenv/.env.local config/script/generate-prisma-client.ts"
  },
  "dependencies": {
    "@fxts/core": "^0.24.0",
    "@nestia/core": "^2.5.15",
    "@nestia/e2e": "^0.4.1",
    "@nestia/fetcher": "^2.5.15",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@toss/nestjs-aop": "^2.1.4",
    "jsonwebtoken": "^9.0.2",
    "nestjs-cls": "^4.2.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typia": "^5.5.4"
  },
  "devDependencies": {
    "@nestia/sdk": "^2.5.15",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-const": "^0.0.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestia": "^5.3.0",
    "prettier": "^3.0.0",
    "prisma": "^5.11.0",
    "prisma-markdown": "^1.0.9",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.2",
    "typescript-transform-paths": "^3.4.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
